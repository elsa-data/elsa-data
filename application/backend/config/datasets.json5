{
  datasets: [
    {
      uri: "urn:example:elsa-data-demo-dataset-smartie",
      name: "Smartie",
      description: "Smartie",
      loader: "australian-genomics-directories",
      storageLocation: "aws-s3",
      // this URL needs to match the constants defined in insert-test-data-smartie - though importantly the
      // bucket itself is NOT REAL - this data all comes via mocks when we are in dev
      storageUriPrefix: "s3://elsa-data-demo-datasets/Smartie",
      // attempt to derive specimen id from filenames
      specimenIdentifierRegex: "^.*([A-Z][A-Z][A-Z])[._].*$",
    },
    {
      uri: "urn:fdc:umccr.org:2022:dataset/10g",
      name: "UMCCR 10G",
      description: "10G",
      loader: "dev",
    },
    {
      uri: "urn:fdc:umccr.org:2022:dataset/10f",
      name: "UMCCR 10F",
      description: "10F",
      loader: "dev",
    },
    {
      uri: "urn:fdc:umccr.org:2022:dataset/10c",
      name: "UMCCR 10C",
      description: "10C",
      loader: "dev",
    },
    {
      uri: "urn:fdc:mackenziesmission.org.au:2022:dataset/vcgs",
      name: "Mackenzie's Mission VCGS",
      description: "The Mackenzie's Mission study sequenced by VCGS",
      loader: "australian-genomics-directories",
      storageLocation: "aws-s3",
      storageUriPrefix: "s3://agha-gdr-store-2.0/MM_VCGS/",
      caseIdentifier: {
        pathRegex: ".*(CPL\\d+).*$",
      },
      specimenIdentifier: {
        pathRegex: ".*(\\d\\dGR\\d+).*$",
      },
    },
    {
      uri: "urn:fdc:mackenziesmission.org.au:2022:dataset/nsw-health",
      name: "Mackenzie's Mission NSW Health",
      description: "The Mackenzie's Mission study sequenced by NSW Health",
      loader: "australian-genomics-directories",
      storageLocation: "aws-s3",
      storageUriPrefix: "s3://agha-gdr-store-2.0/MM_NSWHP/",
      manifestEndsWith: "manifest.orig",
      caseIdentifier: {
        manifestColumnName: "NSWHP_Family_ID",
      },
      specimenIdentifier: {
        manifestColumnName: "sample_id",
      },
    },
  ],
}
