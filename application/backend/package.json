{
  "name": "elsa-data-backend",
  "version": "0.0.0",
  "description": "The backend that serves HTML/API for Elsa Data",
  "author": {
    "name": "University of Melbourne Centre for Cancer Research",
    "url": "https://umccr.org",
    "organization": true
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": " cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production ts-node -r dotenv/config src/bootstrap-local.ts",
    "//typecheck": "Use the Typescript compiler for a compile of the complete src/test/types (no code generated or run)",
    "typecheck": "tsc --noEmit",
    "//edgemigration": "Creates a migration script based on the difference between the current db and schema file",
    "edgemigration": "edgedb migration create",
    "//edgemigrate": "Brings the current db up to date",
    "edgemigrate": "edgedb migrate",
    "//edgetypes": "Generates Typescript classes that match the schema of the *current* project EdgeDb",
    "edgetypes": "edgeql-js",
    "//test": "Run Jest across the full test suite",
    "test": "jest --forceExit --detectOpenHandles --runInBand",
    "//postinstall": "",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.128.0",
    "@aws-sdk/client-s3": "3.128.0",
    "@aws-sdk/client-sts": "3.128.0",
    "@aws-sdk/s3-request-presigner": "3.128.0",
    "@breejs/ts-worker": "^2.0.0",
    "@fastify/cookie": "6.0.0",
    "@fastify/cors": "7.0.0",
    "@fastify/formbody": "6.0.1",
    "@fastify/helmet": "8.0.1",
    "@fastify/jwt": "5.0.1",
    "@fastify/secure-session": "4.1.1",
    "@fastify/static": "5.0.2",
    "@sinclair/typebox": "0.24.18",
    "@umccr/elsa-strings": "file:../common/elsa-strings",
    "@umccr/elsa-types": "file:../common",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "archiver": "5.3.1",
    "archiver-zip-encrypted": "1.0.11",
    "axios": "0.27.2",
    "bree": "9.1.1",
    "csv-stringify": "^6.1.3",
    "date-fns": "2.28.0",
    "dotenv": "9.0.2",
    "edgedb": "0.20.10",
    "esbuild": "0.14.49",
    "fastify": "3.29.1",
    "fastify-favicon": "3.2.0",
    "http-link-header": "1.0.5",
    "humanize-plus": "1.8.2",
    "jszip": "3.10.0",
    "lodash": "4.17.21",
    "openid-client": "5.1.8",
    "patch-package": "6.4.7",
    "qs": "6.11.0",
    "reflect-metadata": "0.1.13",
    "tsyringe": "4.7.0"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/http-link-header": "^1.0.3",
    "@types/jest": "27.5.2",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.38",
    "@types/qs": "^6.9.7",
    "@types/supertest": "2.0.12",
    "cross-env": "7.0.3",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "supertest": "6.2.3",
    "ts-jest": "28.0.4",
    "ts-node": "10.8.1",
    "typescript": "4.7.3"
  }
}
