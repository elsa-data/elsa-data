{
  "name": "backend",
  "version": "0.0.0",
  "description": "The backend that serves HTML/API",
  "author": {
    "name": "University of Melbourne Centre for Cancer Research",
    "url": "https://umccr.org",
    "organization": true
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": " cross-env NODE_ENV=development nodemon",
    "prod": "cross-env NODE_ENV=production ts-node -r dotenv/config src/bootstrap-local.ts",
    "typecheck": "tsc --noEmit",
    "//edgemigration": "Creates a migration script based on the difference between the current db and schema file",
    "edgemigration": "edgedb migration create",
    "//edgemigrate": "Brings the current db up to date",
    "edgemigrate": "edgedb migrate",
    "//edgetypes": "Generates Typescript classes that match the schema of the *current* project EdgeDb",
    "edgetypes": "edgeql-js",
    "test": "jest --forceExit --detectOpenHandles",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@fastify/cors": "7.0.0",
    "@fastify/helmet": "8.0.1",
    "@fastify/jwt": "5.0.1",
    "@fastify/static": "5.0.2",
    "@sinclair/typebox": "0.23.5",
    "@umccr/elsa-types": "file:../common",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "axios": "0.27.2",
    "date-fns": "2.28.0",
    "dotenv": "9.0.2",
    "edgedb": "0.20.10",
    "esbuild": "0.14.39",
    "fastify": "3.29.0",
    "fastify-favicon": "^3.1.0",
    "humanize-plus": "^1.8.2",
    "lodash": "4.17.21",
    "openid-client": "5.1.6",
    "patch-package": "^6.4.7"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.36",
    "@types/supertest": "^2.0.12",
    "cross-env": "7.0.3",
    "nodemon": "2.0.16",
    "prettier": "2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  }
}
